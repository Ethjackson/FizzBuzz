        "inp = input(\"what number do you with to count up to? \\n\")\n",
        "N = (int(inp))\n",
        "\n",
        "for i in range(0, N + 1):\n",
        "  if (i%3 == 0 and i%5 == 0):\n",
        "    print(i,\"fizzbuzz\")\n",
        "  elif (i%3) == 0:\n",
        "    print(i,\"fizz\")\n",
        "  elif (i%5) == 0:\n",
        "    print(i,\"buzz\")\n",
        "  else:\n",
        "    print(i)"
